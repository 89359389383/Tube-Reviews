<h1>Your Favorites</h1>

<ul>
  <% @favorites.each do |favorite| %>
    <li>
      <%= link_to video_path(favorite.video) do %>
        <img src="<%= favorite.video.thumbnail_url %>" alt="<%= favorite.video.title %> thumbnail">
        <%= favorite.video.title %>
      <% end %>
    </li>
  <% end %>
</ul>
